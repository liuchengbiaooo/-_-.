.zan-dialog__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1010;
    background: rgba(0, 0, 0, 0.8);
    /*设置阴影半透明背景如： background: rgba(0, 0, 0, 0.4);*/
    display: none;
}

.zan-dialog__container {
    position: fixed;
    bottom: 580rpx;
    width: 100%;
    /*//弹窗布局宽*/
    height: 380rpx;
    /*//弹窗布局高，与下面弹出距离transform有关*/
    margin-left: 0;
    transform: translateY(300%);
    /*//弹框弹出距离，与弹框布局高度有关，如300%表示弹起距离为3倍弹窗高度*/
    transition: all 0.4s ease;
    z-index: 1012;
    border-radius: 20rpx;
    /*box-shadow: 0px 3px 3px 2px rgb(214, 129, 129);*/
    /*//弹框的悬浮阴影效果，如不需要可注释该行*/
}

.zan-dialog__container {
    font-size: 12px;
    color: #FFD700;
    text-align: center;
}

.zan-dialog--show .zan-dialog__container {
    transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
    display: block;
}

