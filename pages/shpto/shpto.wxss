.weui-msg {
    padding-top: 36px;
    text-align: center;
}
.weui-msg__icon-area {
    margin-bottom: 30px;
}
.weui-msg__text-area {
    margin-bottom: 25px;
    padding: 0 20px;
}
.weui-msg__desc, .weui-msg__title {
    word-wrap: break-word;
    word-break: break-all;
}
.weui-msg__title {
    margin-bottom: 5px;
    font-weight: 400;
    font-size: 20px;
}
.weui-msg__opr-area {
    margin-bottom: 25px;
}
.weui-btn-area {
    margin: 1.17647059em 15px .3em;
}
.brandList_con {
    text-align: center;
    color: #f00;
    margin-top: 20px;
}
.weui-cell_access {
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    color: inherit;
}
.weui-cell {
    padding: 10px 15px;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    background-color:#fff;
    

}
.weui-cell:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    z-index: 2;
}
.weui-cell__bd {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.add_default {
   padding: 10px 15px;
   background-color:#fff;
   border-top: 1px solid #e5e5e5;
   border-bottom: 1px solid #e5e5e5;
